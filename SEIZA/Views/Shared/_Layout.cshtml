<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/Content/assets/styles.css"/>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
    <link href="~/Content/assets/bootstrap/bootstrap.min.css" rel="stylesheet">
    @Scripts.Render("~/bundles/modernizr")
    <style>
        .noScroll {
            background: transparent !important;
            transition: 0.75s ease;
        }

        .scrolled {
            background: rgb(20 20 20 / 0.95) !important;
            height: 3.0rem;
        }   
    </style>

</head>
<body>
    <nav id="#navbar" class="navbar navbar-expand-lg navbar-dark fixed-top">
        <div class="container-fluid">
            <a class="navbar-brand" href="~/Home/Index"><img src="~/Content/assets/images/Name.png" height="44"></a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <!------------------------------------------>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item active">
                        <a class="nav-link" href="~/Home/Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Designs">Designs</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Info">Info</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Home/Contact">Contact</a>
                    </li>
                    <li class="nav-item">
                        <div class="bagContainer">
                            <a class="navbar-link bagLogo" href="~/Home/Cart">
                                <img id="bagLogo" src="~/Content/assets/images/bagLogoInvert.png" height="26">
                                @if (ViewBag.cartCount > 0)
                                {
                                    <p id="notification">@ViewBag.cartCount</p>
                                }
                            </a>
                        </div>
                        
                        
                    </li>
                </ul>
            </div>
        </div>
    </nav>

